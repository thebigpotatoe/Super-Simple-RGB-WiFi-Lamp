void webServerInit() {
  // Set the URI's of the server
  restServer.onNotFound(serve404);
  restServer.on("/", servePage);
  restServer.begin();

  // Set up OTA on the server
  otaInit();

  // Debug
  Serial.println("[webServerInit] - Webserver was set up correctly");
}

void serve404() {
  restServer.send(404, "text/html", ""); 
}

